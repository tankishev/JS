{
    "eslint.options": {
        "envs": {
            "es6": true,
            "browser": true,
            "node": true,
            "mocha": true
        },
        "globals": {
            "expect": true
        },
        "parserOptions": {
            "ecmaVersion": 11,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "rules": {
            "semi": "error",
            "curly": "error",
            "quotes": [
                "warn",
                "single"
            ],
            "no-undef": "error"
        }
    },
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
          "source": "PowerShell",
          "icon": "terminal-powershell",
          "args": ["-ExecutionPolicy", "Bypass"]
        }
      },
      "terminal.integrated.defaultProfile.windows": "PowerShell"
}